#videoGallery

    include video-gallery-nav

    .container-fluid.content
        .slider(style="background:url('img/poisidreas.jpg') top;background-size:cover;")
        //- Pilt vahetatav UNITASe jaoks
        .subnav
            .row
                .col-md-8.col-sm-12.filters
                    .row
                        .col-md-4.col-sm-4.col-xs-6.titleImage
                            img.img-responsive(src='img/Teek.png')
                        .col-md-8.col-sm-8.col-xs-6
                            .sort
                                .images
                                    .instruction.hidden-xs
                                    a.sortby(href='')
                                        img.img-responsive(src='img/kasutaja.svg')
                                    a.sortby(href='')
                                        img.img-responsive(src='img/kalender.svg')
                                .text
                                    .instruction.hidden-xs Reasta:
                                    a.sortby(href='') enim vaadatud
                                    a.sortby(href='') viimati sisestatud
                .col-md-4.col-sm-12.searchcolumn
                    .search
                        input(type='text', placeholder='OTSI')
            .row.dropdowns
                .col-md-4.col-sm-4.col-xs-4.topic
                    select.form-control
                        option.placeholder(selected='', disabled='', value='') Vali teema
                        option(value='') Valik 1
                        option(value='') Valik 2
                        option(value='') Valik 3
                        option(value='') Valik 4
                        option(value='') Valik 5
                .col-md-4.col-sm-4.col-xs-4.area
                    select.form-control
                        option.placeholder(selected='', disabled='', value='') Vali piirkond
                        option(value='') Valik 1
                        option(value='') Valik 2
                        option(value='') Valik 3
                        option(value='') Valik 4
                        option(value='') Valik 5
                .col-md-4.col-sm-4.col-xs-4.generation
                    select.form-control
                        option.placeholder(selected='', disabled='', value='') Vali põlvkond
                        option(value='') Valik 1
                        option(value='') Valik 2
                        option(value='') Valik 3
                        option(value='') Valik 4
                        option(value='') Valik 5
